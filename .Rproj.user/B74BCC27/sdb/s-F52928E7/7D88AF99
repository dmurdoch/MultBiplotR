    "contents" : "# Prácticas de SPSS (ahora con R)\n\nload(\"vinos.Rdata\")\n\nsummary(Vinos$grado)\n\nattach(Vinos)\n\nsummary(grado)\n\ninstall.packages(\"doBy\")\nlibrary(doBy)\n\nsummaryBy(grado~denomina, data=Vinos, FUN=c(mean, sd)\nsummaryBy(grado~denomina, data=Vinos, FUN=c(min, max, mean, sd, median))\n\nplot(grado~denomina)\nboxplot(grado~denomina, notch=TRUE)\n\ninstall.packages(\"ggplot2\")\nlibrary(ggplot2)\nqplot(denomina, grado, data = Vinos, geom = \"boxplot\")\nqplot(denomina, grado, data = Vinos, geom = \"boxplot\", fill=\"red\")\n\nhist(grado)\nhist(grado, breaks=3)\n\nqplot(grado, data = Vinos, geom = \"histogram\", binwidth=0.5)\n\nstem(grado)\n\nxg <- split(grado, denomina)\nboxplot(xg, col = \"lavender\", notch = TRUE, varwidth = TRUE)\nxg\n\nop <- par(mfrow=c(2,1))\nhist(xg$RIBERA, xlim=c(10,14))\nhist(xg$TORO, xlim=c(10,14))\npar(op)\n\nhistBy <- function(x, f, ...){\n  # Niveles del Factor\n  niveles=levels(f)\n  nniv=length(niveles)\n  # Dividimos con los grupos\n  xg <- split(x, f)\n  # Calculo del máximo y del mínimo\n  maximo=max(x)\n  minimo=min(x)\n  # Colocamos los subgráficos\n  op <- par(mfrow=c(nniv,1))\n  # Dibujamos los histogramas\n  for (i in 1:nniv)\n    hist(xg[[i]], main=niveles[i], xlim=c(minimo, maximo), ...)\n  # Devolvemos el gráfico al estado original\n  par(op)\n}\n\nhistBy(grado, denomina)\n\nhistBy(grado, denomina, xlab=\"Grado Alcohólico\")\n\nlibrary(lattice)\nhistogram(grado)\n\nhistByLat <- function(x, f, ...){\n  # Calculo del máximo y del mínimo\n  maximo=max(x)\n  minimo=min(x)\n  # Dibujamos los histogramas\n    histogram(~x|f, endpoints=c(minimo, maximo), ...)\n}\n\nhistByLat(grado, denomina, col=\"green\")\n\n\nhistBy2 <- function(x, f, UseLattice=TRUE, ...){\n  maximo=max(x)\n  minimo=min(x)\n  if (UseLattice)\n    histogram(~x|f, endpoints=c(minimo, maximo), ...)\n  else{\n    niveles=levels(f)\n    nniv=length(niveles)\n    xg <- split(x, f)\n    op <- par(mfrow=c(nniv,1))\n    for (i in 1:nniv)\n      hist(xg[[i]], main=niveles[i], xlim=c(minimo, maximo), ...)\n    par(op)\n  }\n}\n\nhistBy2(grado, denomina, UseLattice=TRUE, col=\"green\")\nhistBy2(grado, denomina, UseLattice=FALSE, col=\"green\")\n\nhistogramBy <- function(x, f, UseLattice=TRUE, Vertical= TRUE, ...){\n  maximo=max(x)\n  minimo=min(x)\n  niveles=levels(f)\n  nniv=length(niveles)\n  \n  if (UseLattice){\n    if (Vertical)\n      histogram(~x|f, endpoints=c(minimo, maximo), layout=c(1,nniv), ...)\n    else\n      histogram(~x|f, endpoints=c(minimo, maximo), layout=c(nniv,1), ...)}\n  else{\n    xg <- split(x, f)\n    if (Vertical)\n      op <- par(mfrow=c(nniv,1))\n    else\n      op <- par(mfrow=c(1,nniv))\n    for (i in 1:nniv)\n      hist(xg[[i]], main=niveles[i], xlim=c(minimo, maximo), ...)\n    par(op)\n    \n  }\n}\n\nhistogramBy(grado, denomina, UseLattice=FALSE, Vertical=FALSE, xlab=\"Grado Acohólico\")\nhistogramBy(grado, denomina, UseLattice=FALSE, Vertical=TRUE, xlab=\"Grado Acohólico\")\nhistogramBy(grado, denomina, UseLattice=TRUE, Vertical=FALSE, xlab=\"Grado Acohólico\")\nhistogramBy(grado, denomina, UseLattice=TRUE, Vertical=TRUE, xlab=\"Grado Acohólico\")\n\nsource('histogramBy.R')\n\n\n################################# CONNTRASTES DE HIPOTESIS\ninstall.packages(\"foreign\")\nlibrary(foreign)\nlibrary(ggplot2)\n\nosteo=read.spss(\"osteoporosis.sav\", to.data.frame =TRUE)\nhead(osteo)\ncolnames(osteo)\nb\n\n# PARTE 2 DE LA PRÁCTICA - SELECCIONAMOS LAS PACIENTES CON OVARIECTOMIA O HISTERECTOMIA\n\nattach(osteo)\nhistogramBy(bua, clasific, UseLattice=FALSE, Vertical=TRUE, xlab=\"BUA\")\n\nqplot(clasific, bua, data = osteo, geom = \"boxplot\")\n\ntapply(bua,clasific,shapiro.test)\n\n\n\n\n(tipo_men==\"OVARIECTOMIA\" | tipo_men==\"HISTERECTOMIA\")\n\n\n",
