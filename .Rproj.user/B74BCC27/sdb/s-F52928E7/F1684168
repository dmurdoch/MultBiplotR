    "contents" : "histogramBy <- function(x, f, UseLattice=TRUE, Vertical= TRUE, ...){\n  maximo=max(x)\n  minimo=min(x)\n  niveles=levels(f)\n  nniv=length(niveles)\n  \n  if (UseLattice){\n    if (Vertical)\n      histogram(~x|f, endpoints=c(minimo, maximo), layout=c(1,nniv), ...)\n    else\n      histogram(~x|f, endpoints=c(minimo, maximo), layout=c(nniv,1), ...)}\n  else{\n    xg <- split(x, f)\n    if (Vertical)\n      op <- par(mfrow=c(nniv,1))\n    else\n      op <- par(mfrow=c(1,nniv))\n    for (i in 1:nniv)\n      hist(xg[[i]], main=niveles[i], xlim=c(minimo, maximo), ...)\n    par(op)\n    \n  }\n}",
